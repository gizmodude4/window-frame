<service>
	<id>windowframe.exe</id>
	<name>Window Frame</name>
	<description>The nodejs server for Window Frame.</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<logmode>rotate</logmode>
	<argument>--harmony</argument>
	<argument>C:\Users\Window\AppData\Roaming\npm\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>C:\Users\Window\window-frame\node\server.js</argument>
	<argument>--log</argument>
	<argument>Window Frame wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<stoptimeout>30sec</stoptimeout>
	<env name="CONFIG" value="C:\Users\Window\window-frame\node\playlist.json"/>
	<workingdirectory>C:\Users\Window\window-frame\node</workingdirectory>
</service>